function CreateHint_radio() {
    local hint = Entities.CreateByClassname("Env_instructor_hint");
    hint.__KeyValueFromString("hint_static", "0");
    hint.__KeyValueFromString("hint_color", "113 145 64");
    hint.__KeyValueFromString("hint_nooffscreen", "0");
    hint.__KeyValueFromString("hint_icon_onscreen", "icon_tip");
    hint.__KeyValueFromString("hint_timeout", "4");
    hint.__KeyValueFromString("hint_forcecaption", "1");
    return hint;
}

function CreateHint_player() {
    local hint = Entities.CreateByClassname("Env_instructor_hint");
    hint.__KeyValueFromString("hint_static", "1");
    hint.__KeyValueFromString("hint_color", "255 255 255");
    hint.__KeyValueFromString("hint_nooffscreen", "0");
    hint.__KeyValueFromString("hint_icon_onscreen", "icon_fire");
    hint.__KeyValueFromString("hint_timeout", "3.5");
    return hint;
}

function CreateText() {
    local text = Entities.CreateByClassname("game_text");
    text.__KeyValueFromString("message","game_text");
    text.__KeyValueFromString("color","238 0 0");
    text.__KeyValueFromString("color2","255 255 255");
    text.__KeyValueFromString("effect","0");
    text.__KeyValueFromString("x","0.2");
    text.__KeyValueFromString("y","0.2");
    text.__KeyValueFromString("channel","2");
    text.__KeyValueFromString("spawnflags","1");
    text.__KeyValueFromString("holdtime","3.5");
    text.__KeyValueFromString("fadein","0.5");
    text.__KeyValueFromString("fadeout","0.5");
    text.__KeyValueFromString("fxtime","0");
    return text;
}

function hint_display(target,example){
    if(target != null){
        local hint = CreateHint_radio();
        EntFireByHandle(hint, "AddOutput", "hint_target "+target, 0.0, null, null);
        hint.__KeyValueFromString("hint_caption", example);
        EntFireByHandle(hint, "ShowHint", "", 0, null, null);
        EntFireByHandle(hint, "kill", "", 4, null, null);
    }
    else{
        local hint = CreateHint_player();
        hint.__KeyValueFromString("hint_caption", example);
        EntFireByHandle(hint, "ShowHint", "", 0, null, null);
        EntFireByHandle(hint, "kill", "", 4, null, null);
    }
}

function credits_display() {
    local delay = 0;
    for(local j=0;j<text.credits.len();j++)
    {
        local Gtext = CreateText();
        if(j%2!=0) Gtext.__KeyValueFromString("y","0.6");
        EntFireByHandle(Gtext, "SetText", text.credits[j], delay, null, null)
        EntFireByHandle(Gtext, "Display", "", delay, null, null);
        EntFireByHandle(Gtext, "kill", "", delay, null, null);
        delay+=5;
    }
    // local value = VS::GetAllPlayers().name;
